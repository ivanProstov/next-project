{
  "name": "next-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "lint": "next lint",
    "dev": "next dev",
    "build:next": "next build",
    "start": "next start",
    "build:server": "npm run clean:server && tsc -p ./tsconfig.json",
    "start:server": "node ./build/server/server.js",
    "clean:server": "rimraf --path ./dist && rimraf --path ./build",
    "start:dev:old": "npm run build:server && node ./build/server/server.js",
    "start:dev:new": "nodemon --watch server --ext ts,js --exec \"npm-run-all build:server start:server\"",
    "start:dev": "nodemon --watch server --ext ts,js --exec \"npm run build:server && npm run start:server\""
  },
  "dependencies": {
    "@types/express": "4.17.21",
    "@types/next": "8.0.7",
    "axios": "1.7.7",
    "babel-plugin-styled-components": "2.1.4",
    "bcrypt": "5.1.1",
    "browserify-zlib": "0.2.0",
    "connect-mongo": "5.1.0",
    "cookie-parser": "1.4.6",
    "express": "4.19.2",
    "express-session": "1.18.0",
    "mongoose": "8.6.1",
    "next": "14.2.8",
    "react": "^18",
    "react-dom": "^18",
    "stream-browserify": "3.0.0",
    "styled-components": "6.1.13",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "util": "0.12.5"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/cookie-parser": "1.4.7",
    "@types/express-session": "1.18.0",
    "@types/node": "20.16.5",
    "@types/react": "18.3.5",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.8",
    "nodemon": "3.1.4",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "ts-loader": "9.5.1",
    "typescript": "5.6.2"
  },
  "type": "commonjs"
}
